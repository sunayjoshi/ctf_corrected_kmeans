python run_clustering_old.py --clustering_type l2 --n_angles 200 --k 150 --ncores 38 --snr 8 --effects=-ctf-old

# snr 1/12
python run_clustering_old.py --clustering_type wemd --n_angles 200 --k 150 --ncores 38 --snr 12 --effects=-ctf-old

python run_clustering_old.py --clustering_type l2 --n_angles 200 --k 150 --ncores 38 --snr 12 --effects=-ctf-old

# snr 1/16
python run_clustering_old.py --clustering_type wemd --n_angles 200 --k 150 --ncores 38 --snr 16 --effects=-ctf-old

python run_clustering_old.py --clustering_type l2 --n_angles 200 --k 150 --ncores 38 --snr 16 --effects=-ctf-old
